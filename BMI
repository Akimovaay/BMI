

print( " What's your weight? " )
w = input()
print( " What's your height? " )
h = input()

BMI = float(w) / float(h) ** 2

if BMI < 18.5:
    print( " Underweight: < 18.5 " )
elif BMI >= 18.5 and BMI < 25:
    print( " Normal: 18.5 - 25 " )
elif BMI >= 25 and BMI < 30:
    print( " Overweight: 25 - 30 " )
else:
    print( " Obesity: > 30 " )
    
print(BMI)